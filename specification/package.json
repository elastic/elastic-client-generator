    "ntypescript": "1.201706190042.1",
    "tslint": "^5.10.0",
    "typescript": "^2.9.2",
    "typings": "0.6.8"
  },
  "dependencies": {
    "glob": "^7.1.1",
    "lodash": "^4.17.13"
