{
  "name": "elasticsearch-client-specification",
  "version": "0.0.1",
  "description": "A library that exposes the elasticsearch client specification as a validatable and iteratable source",
  "main": "compiler/index.ts",
  "scripts": {
    "lint": "ts-standard",
    "lint:fix": "ts-standard --fix",
    "format:check": "prettier --check ../specification/",
    "format:fix": "prettier --write ../specification/",
    "imports:fix": "ts-node update-imports.ts",
    "generate-schema": "ts-node index.ts",
    "compile:specification": "tsc --project ../specification/tsconfig.json --noEmit"
  },
  "author": "Elastic",
  "license": "Apache-2.0",
  "devDependencies": {
    "@types/node": "^14.14.21",
    "minimist": "^1.2.5",
    "ora": "^5.4.0",
    "prettier": "2.2.1",
    "ts-node": "^9.1.1",
    "ts-standard": "^10.0.0",
    "typescript": "^4.1.3"
  },
  "dependencies": {
    "chalk": "^4.1.0",
    "safe-stable-stringify": "^1.1.1",
    "semver": "^7.3.4",
    "ts-morph": "^9.1.0"
  },
  "engines": {
    "node": ">=14"
  }
}
